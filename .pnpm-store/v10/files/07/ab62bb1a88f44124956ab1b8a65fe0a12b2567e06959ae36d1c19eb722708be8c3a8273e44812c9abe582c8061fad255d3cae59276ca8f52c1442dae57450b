import { GithubAuthConfig, AuthState, GithubUser } from "../github/types.js";
export declare const defaultConfig: GithubAuthConfig;
export declare const GITHUB_AUTH_URL = "https://github.com/login/oauth/authorize";
export declare const USER_STORAGE_KEY = "github_user_data";
export declare function saveToken(token: string): Promise<void>;
export declare function getToken(): Promise<string | null>;
export declare function removeToken(): Promise<void>;
export declare function saveUser(user: GithubUser): Promise<void>;
export declare function getUser(): Promise<GithubUser | null>;
export declare function removeUser(): Promise<void>;
export declare function getAuthState(): Promise<AuthState>;
export declare function logout(): Promise<void>;
export declare function fetchUserProfile(token: string): Promise<GithubUser>;
