let t = null, e = null;
function o(n) {
  t = n ?? null;
}
function r(n) {
  e = n;
}
async function i() {
  try {
    if (typeof e == "function") {
      const n = await e();
      if (n) return n;
    }
  } catch {
  }
  return t;
}
export {
  i as getProvidedToken,
  o as setToken,
  r as setTokenProvider
