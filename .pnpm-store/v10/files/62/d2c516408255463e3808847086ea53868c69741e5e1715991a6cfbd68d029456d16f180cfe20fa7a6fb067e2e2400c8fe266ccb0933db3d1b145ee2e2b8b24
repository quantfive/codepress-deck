import { ReactNode } from "react";
interface RepositoryData {
    id: number;
    owner: string;
    name: string;
    default_branch: string;
    user_id: string;
    colors?: string[];
    colors_updated_at?: string;
}
interface RepositoryContextType {
    repository: RepositoryData | null;
    colors: string[];
    isLoading: boolean;
    error: string | null;
    refetch: () => Promise<void>;
}
export declare const RepositoryProvider: ({ children }: {
    children: ReactNode;
}) => import("react/jsx-runtime").JSX.Element;
export declare const useRepository: () => RepositoryContextType;
export {};
