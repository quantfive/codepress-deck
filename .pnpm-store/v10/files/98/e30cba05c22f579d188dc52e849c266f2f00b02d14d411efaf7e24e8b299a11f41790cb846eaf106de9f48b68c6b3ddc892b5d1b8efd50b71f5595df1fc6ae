function o(r) {
  if (!r)
    return {};
  const t = r.match(/^(([^:/?#]+):)?(\/\/([^/?#]*))?([^?#]*)(\?([^#]*))?(#(.*))?$/);
  if (!t)
    return {};
  const e = t[6] || "", n = t[8] || "";
  return {
    host: t[4],
    path: t[5],
    protocol: t[2],
    search: e,
    hash: n,
    relative: t[5] + e + n
    // everything minus origin
  };
}
export {
  o as parseUrl
};
