import { jsx as _jsx, Fragment as _Fragment } from "react/jsx-runtime";
import icon from "../../public/icon/32.png";
const MenuOverlay = ({ onToggleEditing, isEditing, }) => {
    return (_jsx(_Fragment, { children: _jsx("button", { onClick: () => onToggleEditing(), className: "fixed bottom-4 right-4 z-[9999] bg-white rounded-full w-12 h-12 flex items-center justify-center shadow-lg cursor-pointer", "aria-label": "Toggle editing menu", children: _jsx("img", { src: typeof icon === "string" ? icon : icon.src, className: `h-6 w-6 transition-all duration-300 ${isEditing ? "filter-none" : "filter grayscale"}` }) }) }));
};
export default MenuOverlay;
