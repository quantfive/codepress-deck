import type { Organization, Repository, ApiKey, ApiKeyResponse, ApiKeyPermissions } from "./types.js";
/**
 * Fetch organizations with repositories
 */
export declare function fetchOrganizationsWithRepositories(): Promise<Organization[]>;
/**
 * Fetch details for a specific repository
 */
export declare function fetchRepositoryDetails(repoId: number): Promise<Repository>;
/**
 * Link repository to a GitHub repository
 */
export declare function linkRepository(options: {
    owner: string;
    repo: string;
    branch?: string;
}): Promise<Repository>;
/**
 * Create a new API key for an organization
 */
export declare function createApiKey(organizationId: number, permissions?: ApiKeyPermissions): Promise<ApiKeyResponse>;
/**
 * Get all API keys for an organization
 */
export declare function getApiKeys(organizationId: number): Promise<ApiKey[]>;
/**
 * Revoke (delete) an API key
 */
export declare function revokeApiKey(organizationId: number, keyId: number): Promise<{
    success: boolean;
    message: string;
}>;
/**
 * Add more GitHub-specific API functions here as needed
 */
